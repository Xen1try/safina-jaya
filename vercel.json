{
    "version": 2,
    "builds": [
        {
            "src": "public/index.php",
            "use": "vercel-php"
        },
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(css|js|images|storage|favicon.ico|favicon.png)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/public/index.php"
        }
    ]
}
